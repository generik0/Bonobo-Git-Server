<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="MembershipService" value="ActiveDirectory" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="AuthenticationProvider" value="Windows" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="ActiveDirectoryDefaultDomain" value="vem.dk"/>
    <add key="ActiveDirectoryBackendPath" value="~\App_Data\ADBackend"/>
    <add key="ActiveDirectoryBackendPath" value="~\App_Data\ADBackend"/>
    <add key="ActiveDirectoryMemberGroupName" value="Domain Users" />
    <add key="ActiveDirectoryTeamMapping" value="Developers=GitTeam" />
    <add key="ActiveDirectoryRoleMapping" value="Administrator=GitAdmins" />
  </appSettings>
</configuration>